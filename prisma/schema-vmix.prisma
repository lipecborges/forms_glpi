generator client {
  provider = "prisma-client-js"
  output   = "./generated/vmix"
  binaryTargets = ["native", "rhel-openssl-1.0.x", "windows"]
}

datasource db {
  provider = "sqlserver"
  url      = env("DEV_DB_VISUAL_MIX_DIG_URL")
}

model ACESSO {
  TIPO        Int     @default(0, map: "DF_ACESSO_TIPO") @db.SmallInt
  FATO        Int     @default(0, map: "DF_ACESSO_FATO") @db.SmallInt
  CAMPO       String  @db.VarChar(50)
  DESCRICAO   String? @db.VarChar(50)
  HABILITACAO Boolean @default(false, map: "DF_ACESSO_HABILITACAO")

  @@id([CAMPO, FATO, TIPO], map: "PK_ACESSO")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model ACESSO_ESPECIAL {
  LOJA        Int?
  DATA        DateTime? @db.SmallDateTime
  TIPO        Int?      @db.TinyInt
  ENTIDADE    Int?
  OPERACAO    Int?      @db.TinyInt
  USUARIO     Float?
  SENHA       Float?
  COMPLEMENTO String?   @db.VarChar(60)

  @@ignore
}

